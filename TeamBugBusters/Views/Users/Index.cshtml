@model IEnumerable<UserRolesViewModel>

@{
    ViewData["Title"] = "Users";
}

<h1 class="display-4">Users</h1>

<table class="table w-75">
    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>